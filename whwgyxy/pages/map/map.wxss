/* pages/map/map.wxss */

.mycontainer{
  width: 100%;
  height: 100%;
  overflow-y: hidden;
  align-items: center;
  justify-content: space-between;
}

.scrollcontainer{
  width: 100%;
  overflow: scroll;
}
.name {
  display: flex;
  background: #eee;
  border: 1rpx solid #ccc;
  border-radius: 2px;
  height: 16px;
  align-items: center;
  justify-content: center;
}

 @keyframes bounce-up {
  25% {transform: translateY(5px);}
  50%, 100% {transform: translateY(0);}
  75% {transform: translateY(-5px);}
 }
 
.animate-bounce-up{ 
  -webkit-animation: bounce-up 2s linear infinite;
  animation: bounce-up 2s linear infinite;}

.vr-go{
  height: 60px;
  width: 60px;
  border-radius: 50%;
  background: #aaa;
  opacity: 0.4;
  font-size: 18px;
  text-align: center;
  position: fixed;
  right: 20rpx;
  bottom: 60rpx
}